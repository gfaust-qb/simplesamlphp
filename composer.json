{
  "name" : "simplesamlphp/simplesamlphp",
  "description" : "A PHP implementation of SAML 2.0 service provider and identity provider functionality. And is also compatible with Shibboleth 1.3 and 2.0.",
  "type" : "project",
  "keywords" : ["saml2", "shibboleth", "aselect", "openid", "oauth", "ws-federation", "sp", "idp"],
  "homepage" : "http://simplesamlphp.org",
  "license" : "LGPL-2.1",
  "authors" : [
    {
      "name" : "Andreas Ã…kre Solberg",
      "email" : "andreas.solberg@uninett.no"
    },
    {
      "name" : "Olav Morken",
      "email" : "olav.morken@uninett.no"
    }
  ],
  "repositories" : [
    {
      "type" : "package",
      "package" : {
        "name" : "robrichards/xmlseclibs",
        "version" : "3.0.0",
        "source" : {
          "url" : "https://github.com/gfaust-qb/xmlseclibs",
          "type" : "git",
          "reference" : "Specified_Exception"
        },
            "autoload": {
                "psr-4": {
                    "RobRichards\\XMLSecLibs\\": "src/"
                }
            }
      }
    },
    {
      "type" : "package",
      "package" : {
        "name" : "simplesamlphp/saml2",
        "version" : "3.0.0",
        "source" : {
          "url" : "https://github.com/gfaust-qb/saml2",
          "type" : "git",
          "reference" : "master"
        },
                  "autoload": {
                      "psr-0": {
                          "SAML2_": "src/"
                      }
                  },
                  "autoload-dev": {
                      "psr-0": {
                          "SAML2_": "tests/"
                      }
                  }
      }
    }

  ],
  "autoload" : {
    "psr-0" : {
      "SimpleSAML" : "lib/"
    },
    "files" : ["lib/_autoload_modules.php"]
  },
  "require" : {
    "php" : ">=5.3",
    "simplesamlphp/saml2" : "3.*",
    "robrichards/xmlseclibs" : "3.*",
    "whitehat101/apr1-md5" : "~1.0"
  },
  "require-dev" : {
    "phpunit/phpunit" : "~3.7",
    "satooshi/php-coveralls" : "dev-master"
  },
  "support" : {
    "issues" : "https://github.com/simplesamlphp/simplesamlphp/issues",
    "source" : "https://github.com/simplesamlphp/simplesamlphp"
  }
}
